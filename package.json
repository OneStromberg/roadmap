{
  "name": "dashbord",
  "version": "0.0.3",
  "description": "Zajno Dashboard Client Application",
  "scripts": {
    "deploy": "yarn gen-client && firebase deploy",
    "parse": "node parseKML.js",
    "start": "cross-env NODE_ENV=production cross-env BUILD_APP_TARGET=SERVER babel-node server",
    "gen-client": "cross-env NODE_ENV=production cross-env BUILD_APP_TARGET=CLIENT webpack -p --output-pathinfo",
    "dev": "cross-env NODE_ENV=development && cross-env BUILD_APP_TARGET=CLIENT webpack-dev-server -p --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8090 --host 0.0.0.0"
  },
